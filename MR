#include<stdio.h>
int input (int *a, int *b, int *c)
{
	printf("enter a no.");
	scanf("%d",a);
	printf("enter a no.");
	scanf("%d",b);
	printf("enter a no.");
	scanf("%d",c);
}
int compare (int a, int b, int c)
{
	int small;
	small= (a<b ? (a<c ? a:c) : b <c ? (b<a ? b:a) : c); 
}
void output(int x,int y,int z,int m)
{
	printf("%d < %d < %d is %d\n",x,y,z,m);
}
int main(void)
{
	int x,y,z,m;
	input (&x,&y,&z);
	m=compare(x,y,z);
    output(x,y,z,m);
}
